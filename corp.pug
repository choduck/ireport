    Date.prototype.yymmdd = function () {
      let mm = `${this.getMonth() + 1}`
      let dd = `${this.getDate()}`
      if(this.getMonth() + 1 < 10)
        mm = `0${this.getMonth() + 1}`
      if(this.getDate() < 10)
        dd = `0${this.getDate()}`
      return `${this.getFullYear().toString().substring(2)}-${mm}-${dd}`
    }    
    Vue.prototype.$scrollTop = function () {
      let element = document.querySelector('body')
      let top = element.offsetTop
      window.scrollTo(0, top)
    }
    let isMobile = "#{mobile}" == "true"
    var v = new Vue({
      el: '#app',
      components: {
      },
      data: {
        themeClass: {mobile: isMobile},
        strings: {
          '국문': {}, '영문': {}
        },
        accordianIndex: 0,
        translateX: 0,
        bannerList1: [
          { src: '/img/partners/통계청-호버.png' },
          { src: '/img/partners/창원특례시-호버.png' },
          { src: '/img/partners/국립환경과학원-호버.png' },
          { src: '/img/partners/KEITI-호버.png' },
          { src: '/img/partners/EPIS-호버.png' },
          { src: '/img/partners/서울대학교-호버.png' },
          { src: '/img/partners/고려대학교-호버.png' },
        ],
        inv: null
      },
      methods: {
        accordian: function (index) {
          this.accordianIndex = index
        },
        inflate: function(currentElement) {
          this.currentElement = currentElement
          this.$scrollTop()
        },
        changeLang: function (lang) {
          this.lang = lang
        },
        
      },
      computed: {
      },
      created()
      {
      },
      mounted()
      {
        let spanWidth = Math.floor(window.innerWidth / 5)
        let bannerList1NextIndex = 0, isPushed = false
        this.inv = setInterval(() => {
          this.translateX++
          if(this.translateX % spanWidth == 0)
          {
            bannerList1NextIndex = (bannerList1NextIndex + 1) % this.bannerList1.length
            isPushed = false
          }
          if(this.translateX % spanWidth > 0 && isPushed == false)
          {
            isPushed = true
            this.bannerList1.push(this.bannerList1[bannerList1NextIndex])
          }
          if(this.translateX > 2222)
          {
            this.translateX = 0
            this.bannerList1 = [
              { src: '/img/partners/통계청-호버.png' },
              { src: '/img/partners/창원특례시-호버.png' },
              { src: '/img/partners/국립환경과학원-호버.png' },
              { src: '/img/partners/KEITI-호버.png' },
              { src: '/img/partners/EPIS-호버.png' },
              { src: '/img/partners/서울대학교-호버.png' },
              { src: '/img/partners/고려대학교-호버.png' },
            ]
          }
        }, 20)
        /*
        http({uri:"글읽기", method:"patch", param: { tanancy: '비전솔루션매니스탈링스', pageName: '메인' }, cb: (res) => {
          if(res.code === 0)
          {
            delete res.code
            this.strings = res
          }
        }})*/

      },
      beforeDestroy()
      {
        if(!!this.inv)
          clearInterval(this.inv)
      }

    })