    let contentListEl = document.querySelector('.content-list')

    Date.prototype.yymmdd = function () {
      let mm = `${this.getMonth() + 1}`
      let dd = `${this.getDate()}`
      if(this.getMonth() + 1 < 10)
        mm = `0${this.getMonth() + 1}`
      if(this.getDate() < 10)
        dd = `0${this.getDate()}`
      return `${this.getFullYear().toString().substring(2)}-${mm}-${dd}`
    }    
    Vue.prototype.$scrollTop = function () {
      let element = document.querySelector('body')
      let top = element.offsetTop
      window.scrollTo(0, top)
    }
    let isMobile = "#{mobile}" == "true"
    var v = new Vue({
      el: '#app',
      components: {
      },
      data: {
        themeClass: {mobile: isMobile},
        strings: {
          '국문': {}, '영문': {}
        },
        tabIndex: 0,
        accordianIndex: 0,
        srcMap: {
          '/img/partners/통계청.png': '/img/partners/통계청-호버.png',
          '/img/partners/창원특례시.png': '/img/partners/창원특례시-호버.png',
          '/img/partners/국립환경과학원.png': '/img/partners/국립환경과학원-호버.png',
          '/img/partners/KEITI.png': '/img/partners/KEITI-호버.png',
          '/img/partners/EPIS.png': '/img/partners/EPIS-호버.png',
          '/img/partners/서울대학교.png': '/img/partners/서울대학교-호버.png',
          '/img/partners/고려대학교.png': '/img/partners/고려대학교-호버.png',
        },
        slideMouseDown: false,
        slideStartX: 0,
        slideScrollLeftStart: 0,
      },
      methods: {
        getSrc: function (src) {
          return this.srcMap[src]
        },
        setSrc: function (newSrc, src) {
          return
          this.srcMap[src] = newSrc
        },
        inflate: function(currentElement) {
          this.currentElement = currentElement
          this.$scrollTop()
        },
        changeLang: function (lang) {
          this.lang = lang
        },
        slide: function (e, type) {
          if(type == 'watch') {
            return
          }
          if(type == 'mousedown')
          {
            this.slideMouseDown = true
            let offsetLeft = contentListEl.offsetLeft
            let scrollLeft = contentListEl.scrollLeft
            this.slideStartX = e.pageX - offsetLeft
            this.slideScrollLeftStart = scrollLeft
          }
          else if(type =='mouseup')
          {
            this.slideMouseDown = false
          }
          else if(type == 'mousemove')
          {
            if(this.slideMouseDown == false) return 
            e.preventDefault()
            let offsetLeft = contentListEl.offsetLeft
            const x = e.pageX - offsetLeft
            const walk = 2 * (x - this.slideStartX)
            contentListEl.scrollLeft = this.slideScrollLeftStart - walk
            this.a = contentListEl.scrollLeft
            this.b = contentListEl.clientWidth
          }
        },        
        tab: function(index) {
          this.tabIndex = index
          this.$nextTick(() => {
            setTimeout(() => {
              document.querySelector(`section.tab${index}`).classList.add('animation')
            }, 0)
            setTimeout(() => {
              document.querySelector(`section.tab${index}`).classList.remove('animation')
            }, 1000)

          })
        }
      },
      computed: {
      },
      created()
      {
      },
      mounted()
      {
        http({uri:"글읽기", method:"patch", param: { tanancy: '비전솔루션매니스탈링스', pageName: '메인' }, cb: (res) => {
          if(res.code === 0)
          {
            delete res.code
            this.strings = res
          }
        }})

      },
      beforeDestroy()
      {
      }

    })