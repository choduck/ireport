			methods: {
				onHover: function (index) {
					this.banner.current = this.bannerList[index]
				},
				getSrc: function (src) {
					return this.srcMap[src]
				},
				setSrc: function (src, newSrc) {
					this.srcMap[src] = newSrc
				},
				slide: function (e, cmd, label) {
					if(cmd == '이전')
					{
						if(this.slideIndex[label] - 1 < 0) return
						this.slideIndex[label] = (this.slideIndex[label] - 1) % this.slides[label].length
						e.target.nextSibling.nextSibling.style.transform = `translateX(-${this.slideIndex[label] * this.slideWidth[label]}px)`
					}
					else if(cmd == '다음')
					{
						if(this.slideIndex[label] + 1 >= this.slides[label].length) return
						this.slideIndex[label] = (this.slideIndex[label] + 1) % this.slides[label].length
						e.target.nextSibling.style.transform = `translateX(-${this.slideIndex[label] * this.slideWidth[label]}px)`
					}
				},
				bigSlide: function (e, cmd, label) {
					let slideWidth = window.innerWidth
					if(cmd == '이전')
					{
						if(this.bigSlideIndex[label] - 1 < 0) return
						this.bigSlideIndex[label] = (this.bigSlideIndex[label] - 1) % this.bigSlides[label].length
						//e.target.nextSibling.nextSibling.style.transform = `translateX(-${this.bigSlideIndex[label] * slideWidth}px)`
					}
					else if(cmd == '다음')
					{
						if(this.bigSlideIndex[label] + 1 >= this.bigSlides[label].length) return
						this.bigSlideIndex[label] = (this.bigSlideIndex[label] + 1) % this.bigSlides[label].length
						//e.target.nextSibling.style.transform = `translateX(-${this.bigSlideIndex[label] * slideWidth}px)`
					}
				},
				setClassName: function (index, bigSlideIndex, maxLength) {
					if(maxLength == 2)
					{
						if(bigSlideIndex == 0)
						{
							if(index == 0)
								return { second: true }
							if(index == 1)
								return { third: true }
						}
						if(bigSlideIndex == 1)
						{
							if(index == 1)
								return { second: true }
							if(index == 0)
								return { third: true }
						}
					}
					if(bigSlideIndex == 0)
					{
						if(index == 2)
							return { first: true }
						if(index == 0)
							return { second: true }
						if(index == 1)
							return { third: true }
					}
					if(bigSlideIndex == 1)
					{
						if(index == 1)
							return { first: true }
						if(index == 2)
							return { second: true }
						if(index == 0)
							return { third: true }
					}
					if(bigSlideIndex == 2)
					{
						if(index == 0)
							return { first: true }
						if(index == 1)
							return { second: true }
						if(index == 2)
							return { third: true }
					}

				},
				inflate: function (currentElement) {
					this.currentElement = currentElement
					this.$scrollTop()
				},
				changeLang: function (lang) {
					this.lang = lang
				},
				tab: function(index) {
					this.tabIndex = index
					this.$nextTick(() => {
						setTimeout(() => {
						document.querySelector(`section.tab${index}`).classList.add('animation')
						}, 0)
						setTimeout(() => {
						document.querySelector(`section.tab${index}`).classList.remove('animation')
						}, 1000)

					})

				}
			},
