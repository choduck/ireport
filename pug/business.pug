									button.slide-next-button-icon(@click="slide($event, '다음', '관광/레저 커뮤니티')")
									.content
										.slide(:class="{current: slideIndex['관광/레저 커뮤니티'] != index}" v-for="slide, index in slides['관광/레저 커뮤니티']")
											img(loading="lazy" :src="slide.src", :alt="slide.alt")
											.img-front
